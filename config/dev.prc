# This is the PRC configuration file for settings that are
# specific to developer instances of Toontown Online.

# Window settings
window-title Toontown Rewritten Custom [DEV]

# Notify settings
console-output true

# Server settings
server-version tto-dev

# Developer settings
want-dev false
schellgames-dev false
exec-chat true
log-private-info true
want-qa-regression true
suits-always-join 1

# Chat settings
want-whitelist false

# Audio settings
audio-library-name p3openal_audio

# Resources settings
vfs-mount resources/ttr_phs_general_01.toontask /
vfs-mount resources/ttr_phs_toon_01.toontask /
vfs-mount resources/ttr_phs_toon_02.toontask /
vfs-mount resources/ttr_phs_toon_03.toontask /
vfs-mount resources/ttr_phs_toon_04.toontask /
vfs-mount resources/ttr_phs_sellbot_01.toontask /
vfs-mount resources/ttr_phs_cashbot_01.toontask /
vfs-mount resources/ttr_phs_lawbot_01.toontask /
vfs-mount resources/ttr_phs_bossbot_01.toontask /
vfs-mount resources/ttr_phs_battle_01.toontask /
vfs-mount resources/ttr_phs_suit_01.toontask /
vfs-mount resources/ttr_phs_event_01.toontask /
vfs-mount resources/ttr_phs_estate_01.toontask /
vfs-mount resources/ttr_phs_kaboomberg_01.toontask /
vfs-mount resources/ttr_phs_media_01.toontask /
vfs-mount resources/ttr_phs_ttc_01.toontask /



# DC file
dc-file astron/dclass/tto.dc
